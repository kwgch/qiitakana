%h1 Listing profiles
%table
  %thead
    %tr
      %th First name
      %th Last name
      %th Website url
      %th Location
      %th Description
      %th Facebook
      %th Linkedin
      %th Google plus
      %th{:colspan => "3"}
  %tbody
    - @profiles.each do |profile|
      %tr
        %td= profile.first_name
        %td= profile.last_name

        %td= link_to 'Show', user_profile_path(current_user, profile)
        %td= link_to 'Edit', edit_user_profile_path(current_user, profile)
%br/
= link_to 'New Profile', new_user_profile_path(current_user)
